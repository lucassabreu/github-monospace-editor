{
    "version": "0.1",
    "manifest_version": 2,
    "name": "Monospace GitHub Editor",
    "description": "Thank god it exists !!!",
    "browser_action": {
        "default_title": "Monospace GitHub Editor"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "*://github.com/*"
    ],
    "background": {
        "persistent": false,
        "scripts": [
            "background.js"
        ]
    },
    "web_accessible_resources": [
        "github-extra.css"
    ]
}